  "license": "MIT",
  "files": [
    "src"
  ],
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^2.0.0",
    "mz": "^1.0.2"
  },
  "engines": {
    "node": ">= 7.0.0"
  },
  "dependencies": {
    "vinyl": "^0.5.3"
